<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5FDFF">

    <TextView
        android:id="@+id/CV"
        android:layout_width="196dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:text="CV.PDF"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/numberuser"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView401"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Phone number"
        android:textColor="#00B0FF"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="@+id/text404"
        app:layout_constraintTop_toBottomOf="@+id/email" />

    <TextView
        android:id="@+id/numberuser"
        android:layout_width="302dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:text="09876567890"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/email"
        app:layout_constraintTop_toBottomOf="@+id/textView401" />

    <TextView
        android:id="@+id/text404"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="260dp"
        android:fontFamily="@font/poppins_medium"
        android:text="E-mail"
        android:textColor="#00B0FF"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/email"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:fontFamily="@font/poppins_medium"
        android:text="email@email.com"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text404" />

    <Button
        android:id="@+id/Logout"
        android:layout_width="217dp"
        android:layout_height="52dp"
        android:layout_marginStart="97dp"
        android:layout_marginTop="608dp"
        android:layout_marginEnd="48dp"
        android:backgroundTint="#6E00B0FF"
        android:text="LOGOUT"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins_medium"
        android:text="CV"
        android:textColor="#00B0FF"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="@+id/textView401"
        app:layout_constraintTop_toBottomOf="@+id/numberuser" />

    <ImageView
        android:id="@+id/imageView169"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="500dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_plus" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="165dp"
        android:layout_marginTop="169dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Aness"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/teteprofile" />

    <ImageView
        android:id="@+id/modifypicture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="242dp"
        android:layout_marginTop="117dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/modifyfoto" />

    <include
        android:id="@+id/bottom_inscription"
        layout="@layout/bottominscri"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>